{
  "name": "link-saver-bun-webview-impl",
  "module": "index.ts",
  "type": "module",
  "author": {
    "name": "Edgar Lopez Calomarde"
  },
  "private": true,
  "scripts": {
    "develop:idx": "NODE_ENV=development bun --watch src/index.ts",
    "develop:app": "NODE_ENV=development bun --watch src/app.ts",
    "prod": "NODE_ENV=production bun src/index.ts",
    "dev": "concurrently 'bun run develop:app' 'bun run develop:idx'"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "^9.2.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/static": "^1.4.4",
    "elysia": "^1.4.12",
    "webview-bun": "^2.4.0"
  }
}
